{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020"],
    "module": "commonjs",
    "jsx": "react-native",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true
  },
  "exclude": ["node_modules"]
}
